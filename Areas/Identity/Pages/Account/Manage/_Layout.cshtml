@{
    Layout = "~/Views/Shared/UserLO.cshtml";
    IgnoreSection("Scripts");
}


<div class="card shadow mb-4" style="width: 70%; margin: 0 auto; margin-top: 1.5rem;">
    <div class="card-header py-3">
        <h2 class="text-center m-0 font-weight-bold text-danger" style="padding: 50px 0;">Account settings</h6>
    </div>
    <div class="card-body" style="min-height: 500px; padding-top: 100px;">
        <hr />
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-2">
                <partial name="_ManageNav" />
            </div>
            <div class="col-md-7">
                @RenderBody()
            </div>
        </div>
    </div>
</div>

