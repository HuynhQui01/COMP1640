@model Comp1640.Models.ContributionFeedbackView
@{
    Layout = "~/Views/Shared/UserLO.cshtml";

}

<div>
    <dl class="row">
        <dt class="col-sm-2">
            Contribution Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.contribution.ConName)
        </dd>
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.contribution.Status)
        </dd>
        <dt class="col-sm-2">
            Feedback
        </dt>
        @foreach (var item in Model.feedbacks)
        {
            <dd class="col-sm-10" style="border-bottom: 1px solid black;">
                @Html.DisplayFor(model => item.Comment)
            </dd>
            <dt class="col-sm-2">

            </dt>
        }
    </dl>
</div>