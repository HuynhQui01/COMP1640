@model IEnumerable<Comp1640.Models.Faculty>

@{
    Layout = "~/Views/Shared/CoordinatorLO.cshtml";

}
<style>
    tr>td {
        border-bottom: black solid 1px !important;
    }
</style>

<body>
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-danger text-center" style="font-size: 40px;">View Faculties</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="datatables" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Faculty's Name</th>
                            <th>Close Date</th>
                            <th>Final Close Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.FacName)
                                </td>
                                
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>

</html>
